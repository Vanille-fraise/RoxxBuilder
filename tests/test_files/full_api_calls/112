ante. É engraçado porque também foi a última coisa que você viu antes da sua cabeça acertar o chão.","ru":"Первое, что замечает ваш противник, решив украсть эту цепочку, - ее блеск. И это же будет последним, что увидите вы - за мгновение до того, как ваша голова покатится по полу.","id":51056},"iconId":1166,"level":128,"realWeight":10,"useAnimationId":0,"price":12800,"recipeSlots":8,"craftVisible":"SC!5|ST!5","craftConditional":null,"craftFeasible":"SC!5|ST!5","visibility":null,"importantNoticeId":0,"importantNotice":{"id":0},"changeVersion":null,"tooltipExpirationDate":null,"effects":[{"_id":"62bb0e66e4e8f8001f7f7736","from":101,"to":120,"characteristic":11,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f7737","from":31,"to":40,"characteristic":15,"category":0,"elementId":2},{"_id":"62bb0e66e4e8f8001f7f7738","from":31,"to":40,"characteristic":13,"category":0,"elementId":3},{"_id":"62bb0e66e4e8f8001f7f7739","from":21,"to":30,"characteristic":12,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f773a","from":1,"to":2,"characteristic":18,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f773b","from":1,"to":0,"characteristic":1,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f773c","from":4,"to":6,"characteristic":49,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f773d","from":4,"to":6,"characteristic":89,"category":0,"elementId":2},{"_id":"62bb0e66e4e8f8001f7f773e","from":4,"to":6,"characteristic":90,"category":0,"elementId":3},{"_id":"62bb0e66e4e8f8001f7f773f","from":11,"to":15,"characteristic":48,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f7740","from":7,"to":10,"characteristic":54,"category":1,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f7741","from":7,"to":10,"characteristic":55,"category":1,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f7742","from":4,"to":5,"characteristic":78,"category":0,"elementId":-1},{"_id":"62bb0e66e4e8f8001f7f7743","from":7,"to":10,"characteristic":84,"category":0,"elementId":5}],"slug":{"de