os, Tomo X\"","ru":"[!] Precious Mineral Shard","id":926480},"iconId":16693,"level":200,"realWeight":10,"useAnimationId":0,"price":0,"recipeSlots":8,"craftVisible":null,"craftConditional":null,"craftFeasible":null,"visibility":null,"importantNoticeId":919416,"importantNotice":{"de":"[UNKNOWN_TEXT_ID_919416]","en":"[UNKNOWN_TEXT_ID_919416]","es":"[UNKNOWN_TEXT_ID_919416]","fr":"[UNKNOWN_TEXT_ID_919416]","it":"[UNKNOWN_TEXT_ID_919416]","nl":"[UNKNOWN_TEXT_ID_919416]","pt":"[UNKNOWN_TEXT_ID_919416]","ru":"[UNKNOWN_TEXT_ID_919416]","id":919416},"changeVersion":"","tooltipExpirationDate":1634313671737,"effects":[{"_id":"62bb0eb0e4e8f8001f7fd236","from":351,"to":400,"characteristic":11,"category":0,"elementId":-1},{"_id":"62bb0eb0e4e8f8001f7fd237","from":71,"to":90,"characteristic":13,"category":0,"elementId":3},{"_id":"62bb0eb0e4e8f8001f7fd238","from":71,"to":90,"characteristic":14,"category":0,"elementId":4},{"_id":"62bb0eb0e4e8f8001f7fd239","from":36,"to":45,"characteristic":10,"category":0,"elementId":1},{"_id":"62bb0eb0e4e8f8001f7fd23a","from":36,"to":45,"characteristic":15,"category":0,"elementId":2},{"_id":"62bb0eb0e4e8f8001f7fd23b","from":-16,"to":0,"characteristic":85,"category":0,"elementId":-1},{"_id":"62bb0eb0e4e8f8001f7fd23c","from":5,"to":8,"characteristic":87,"category":0,"elementId":5},{"_id":"62bb0eb0e4e8f8001f7fd23d","from":16,"to":25,"characteristic":12,"category":0,"elementId":-1},{"_id":"62bb0eb0e4e8f8001f7fd23e","from":4,"to":5,"characteristic":79,"category":0,"elementId":-1},{"_id":"62bb0eb0e4e8f8001f7fd23f","from":-10,"to":0,"characteristic":78,"category":0,"elementId":-1},{"_id":"62bb0eb0e4e8f8001f7fd240","from":16,"to":20,"characteristic":90,"category":0,"elementId":3},{"_id":"62bb0eb0e4e8f8001f7fd241","from":16,"to":20,"characteristic":91,"category":0,"elementId":4},{"_id":"62bb0eb0e4e8f8001f7fd242","from":7,"to":10,"characteristic":88,"category":0,"elementId":1},{"_id":"62bb0eb0e4e8f8001f7fd243","from":7,"to":10,"characteristic":89,"category":0